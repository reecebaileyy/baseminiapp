{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "functions": {
    "api/create-session.ts": {
      "maxDuration": 30
    },
    "api/discover.ts": {
      "maxDuration": 300
    },
    "api/tokens.ts": {
      "maxDuration": 30
    },
    "api/tokens/trending.ts": {
      "maxDuration": 30
    },
    "api/tokens/new.ts": {
      "maxDuration": 30
    },
    "api/tokens/[address].ts": {
      "maxDuration": 30
    }
  },
  "crons": [
    { "path": "/api/refresh", "schedule": "0 * * * *" }
  ]
}

